
{{#if members}}

  {{#if members.properties}}
    <h3>properties</h3>
    <ul class="apis members api-members">
      {{#members.properties}}
        <li>
          <a class="name" href="{{parent.project.base}}/{{parent.type}}/{{parent.name}}.html#property_{{name}}">
            {{name}}
          </a>
        </li>
      {{/members.properties}}
    </ul>
  {{/if}}

  {{#if members.events}}
    <h3>events</h3>
    <ul class="apis members api-members">
      {{#members.events}}
        <li>
          <a class="name" href="{{parent.project.base}}/{{parent.type}}/{{parent.name}}.html#event_{{name}}">
            {{name}}
          </a>
        </li>
      {{/members.events}}
    </ul>
  {{/if}}

  {{#if members.attributes}}
    <h3>attributes</h3>
    <ul class="apis members api-members">
      {{#members.attributes}}
        <li>
          <a class="name" href="{{parent.project.base}}/{{parent.type}}/{{parent.name}}.html#attr_{{name}}">
            {{name}}
          </a>
        </li>
      {{/members.attributes}}
    </ul>
  {{/if}}

  {{#if members.methods}}
    <h3>methods</h3>
    <ul class="apis members api-members">
      {{#members.methods}}
        {{# if access}}
        <li>
          <a class="name" href="{{parent.project.base}}/{{parent.type}}/{{parent.name}}.html#method_{{name}}">
            {{name}}
          </a>
        </li>
        {{/if}}
      {{/members.methods}}
    </ul>
  {{/if}}

{{/if}}
