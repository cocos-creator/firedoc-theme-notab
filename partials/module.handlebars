
<h1>
  <span class="type">Module</span>
  <span class="name">{{name}}</span>
</h1>
<div class="box clearfix meta">
  {{#extra}}
    {{#selleck}}
      <a class="button link-docs" href="/yui/docs/{{name}}">User Guide &amp; Examples</a>
    {{/selleck}}
  {{/extra}}

  {{#if requires}}
    <div class="uses">
      Requires
      <ul class="inline commas">
        {{#requires}}
          <li>{{#crossLinkModule .}}{{/crossLinkModule}}</li>
        {{/requires}}
      </ul>
    </div>
  {{/if}}

  {{#if foundAt}}
    <div class="foundat">
      Defined in: <a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a>
    </div>
  {{/if}}

  {{#if process}}
    <div class="processRun">
      Process: <a href="http://docs.fireball-x.com/editor/ipc-channel/#Core_and_Page_Process">{{process}}</a>
    </div>
  {{/if}}

  {{#if since}}
    <p>Available since {{since}}</p>
  {{/if}}

</div>

{{#if deprecated}}
  <div class="box deprecated">
    <p>
      {{#if deprecationMessage}}
        <strong>Deprecated:</strong> {{deprecationMessage}}
      {{else}}
        This module is deprecated.
      {{/if}}
    </p>
  </div>
{{/if}}

<div class="box intro">
  {{{description}}}
</div>

{{#example}}
  <div class="example">
    <h4>Example:</h4>
    <div class="example-content">{{{.}}}</div>
  </div>
{{/example}}

<!-- Class member index -->
{{#if classes.length}}
  <div class="index-section properties">
    <h3 class="section-title">Class</h3>
    <ul>
      {{#classes}}
        <li>
          {{#if is_enum}}
            <a href="{{project.base}}/enums/{{name}}.html">
              {{name}}
            </a>
          {{else}}
            <a href="{{project.base}}/classes/{{name}}.html">
              {{name}}
            </a>
          {{/if}}
        </li>
      {{/classes}}
    </ul>
  </div>
{{/if}}

{{#if subModules.length}}
  <div class="index-section properties">
    <h3 class="section-title">Sub Modules</h3>
    <ul class="index-list properties">
      {{#subModules}}
        <li class="index-item">
          <a href="{{project.base}}/modules/{{name}}.html">
            {{name}}
          </a>
        </li>
      {{/subModules}}
    </ul>
  </div>
{{/if}}
<!--
<h3>Direct Members</h3>
{{> items-index}} -->

<div class="yui3-g">

  {{#if members.properties}}
    <div class="yui3-u-1-2">
      <h3 class="section-title">Properties</h3>
      {{#members.properties}}{{> props}}{{/members.properties}}
    </div>
  {{/if}}

  {{#if members.attributes}}
    <div class="yui3-u-1-2">
      <h3 class="section-title">Attributes</h3>
      {{#members.attributes}}{{> attrs}}{{/members.attributes}}
    </div>
  {{/if}}

  {{#if members.methods}}
    <div class="yui3-u-1-2">
      <h3 class="section-title">Methods</h3>
      {{#members.methods}}{{> method}}{{/members.methods}}
    </div>
  {{/if}}

  {{#if members.events}}
    <div class="yui3-u-1-2">
      <h3 class="section-title">Events</h3>
      {{#members.events}}{{> method}}{{/members.events}}
    </div>
  {{/if}}

</div>
